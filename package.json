  },
  "peerDependencies": {
    "express": ">=4.18",
    "scimmy": "1.x"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "chalk": "^5.3.0",
    "express": "^4.21.2",
    "minimist": "^1.2.8",
    "ostensibly-typed": "^1.1.0",
    "rollup": "^4.28.1",
    "scimmy": "^1.3.1",
    "typescript": "^5.7.2"
