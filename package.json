  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=16"
  },
  "main": "./dist/routers.js",
  "types": "./dist/routers.d.ts",
  "exports": {
    "import": "./dist/routers.js",
    "require": "./dist/routers.cjs"
  },
  "scripts": {
